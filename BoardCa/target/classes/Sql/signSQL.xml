<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="signXml">

<select id="idCheck" resultType="int" parameterType="MemberDto">
	select count(*) 
	from MEMBER 
	where MEM_ID=#{userId}
</select>


<select id="signCheck" resultType="String" parameterType="MemberDto">
	select mem_pw
	from MEMBER
	where MEM_ID=#{userId}
</select>

<insert id="sessionInput" parameterType="Sign.SessionDto">
	
</insert>

<insert id="memInsert" parameterType="MemberDto">
	


</insert>





</mapper>