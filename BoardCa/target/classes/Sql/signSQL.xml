<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="signXml">

<select id="idCheck" resultType="int" parameterType="String">
	select mem_id
	from member 
	where mem_id=#{userId}
</select>

<select id="signCheck" resultType="String" parameterType="MemberDto">
	select mem_pw
	from member
	where mem_id=#{userId}
</select>

<select id="sessionInput" parameterType="Sign.SessionDto">
	
</select>






</mapper>