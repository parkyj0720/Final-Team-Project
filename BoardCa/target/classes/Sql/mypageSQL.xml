<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mypageXml">

	<select id="MemberInfo" resultType="MemberDto"
		parameterType="MemberDto">
		select * from MEMBER_T where MEM_ID=#{mem_id}
	</select>
	<select id="InfoEdit" resultType="MemberDto"
		parameterType="MemberDto">
		update MEMBER_T set
		MEM_NICKNAME=#{mem_nickname},MEM_PW=#{mem_pw},MEM_EMAIL=#{mem_email},MEM_ROCAL=#{mem_rocal},MEM_STATE=#{mem_state}
		where MEM_ID=#{mem_id}

	</select>

</mapper>